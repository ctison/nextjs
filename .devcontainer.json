{
  "build": {
    "dockerFile": "../Dockerfile",
    "target": "dev"
  },
  "forwardPorts": [3000],
  "extensions": [
    "dbaeumer.vscode-eslint",
    "eamodio.gitlens",
    "exiasr.hadolint",
    "esbenp.prettier-vscode",
